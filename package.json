  "main": "server/main.js",
  "proxy": "http://localhost:8080",
  "scripts": {
    "magic": "concurrently \"npm run start\" \"npm run watch\"",
    "server": "npm run build && node server/main.js",
    "start": "forever server/main.js",
    "watch": "react-scripts start",
    "build": "react-scripts build && npm run build-rename",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-rename": "npm run build-rename-js && npm run build-rename-css && npm run build-html-replace",
    "build-rename-js": "renamer --regex --find 'main\\.[^\\.]+\\.js' --replace 'main.js' build/static/js/*.js",
    "build-rename-css": "renamer --regex --find 'main\\.[^\\.]+\\.css' --replace 'main.css' build/static/css/*.css",
    "build-html-replace": "replace 'main\\.[^\\.]+\\.js' 'main.js' build/index.html && replace 'main\\.[^\\.]+\\.css' 'main.css' build/index.html"
  }
